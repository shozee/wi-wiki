      body {
        font-family: sans-serif;
        background-color: #ffffff;
        color: #222222;
        margin: 4.0em;
        line-height: 1.2em;
      }
      h1 {
        font-size: 2.5em;
        line-height: 1.0em;
        padding: 0.3em;
      }
      h2 {
        font-size: 1.5em;
        line-height: 1.0em;
        padding: 0.3em;
        border-bottom: solid 1px #516ab6;
      }
      h3 {
        font-size: 1.2em;
        line-height: 1.0em;
        padding: 0.3em;
      }
      h4 {
        text-decoration: underline;
      }
      hr {
        background-color: #000000;
        height: 2px;
        border: 0;
      }
      a {
        background-color: #00bfff;
        color: #ffffff;
        text-decoration: none;
        padding-top: 0em;
        padding-bottom: 0em;
        padding-left: 0.1em;
        padding-right: 0.1em;
      }
      a:hover {
        background-color: #222222;
        color: #ffffff;
      }
      pre {
        padding: 0.3em;
        margin-left: 20px;
        background-color: #EEEEEE;
        white-space: -moz-pre-wrap; /* Mozilla */
        white-space: -pre-wrap;     /* Opera 4-6 */
        white-space: -o-pre-wrap;   /* Opera 7 */
        white-space: pre-wrap;      /* CSS3 */
        word-wrap: break-word;      /* IE 5.5+ */
      }
      pre code {
        font-family: monospace;
        font-weight: normal;
        text-align: left;
        line-height: 80%;
        margin-bottom: 10px;
      }
      p code {
        padding: 0.3em;
        font-family: monospace;
        font-weight: normal;
        background-color: #EEEEEE;
      }
      td {
        border: solid 1px;
      }
      th {
        border: solid 1px;
        background: #DDDDDD;
      }
      table {
        border-collapse: collapse;
      }
      .MJX-TEX a {
        font-family: sans-serif;
      }
      /* highlight.js */
      .hljs {
        line-height: 1.2em;
        margin     : 0em;
      }
      .hljs-ln-line {
        border: none;
      }
    </style>

